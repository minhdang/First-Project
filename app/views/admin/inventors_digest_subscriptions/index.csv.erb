<%- InventorsDigestSubscription.find_each(:include => [:user, :mailing_address], :batch_size => 500) do |s| -%>
<%= [s.mailing_address.first_name, s.mailing_address.last_name, s.mailing_address.address_1, s.mailing_address.address_2, s.mailing_address.state, s.mailing_address.city, s.mailing_address.postal_code].map{|item| item.gsub(',','')}.join(',') %>
<%- end -%>